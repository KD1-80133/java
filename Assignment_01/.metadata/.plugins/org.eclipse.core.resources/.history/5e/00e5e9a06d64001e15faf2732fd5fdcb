
public class Assignment1_04{
	public class pattern{
	public void patternA(String[] args) {
		int rows = 5;
        for (int i = 1; i <= rows; i++) {
            for (int j = 1; j <= rows-i; j++) {
                System.out.print(" ");
            }
            for (int j = 1; j <= i; j++) {
                System.out.print(" * ");
            }
            System.out.println();
        }
    }
}
public class PatternB{
		public static  main(String[] args)
		{
			int rows=6;
			for(int i=1;i<=rows; i++)
			{
				for(int j=1;j<=i;j++)
				{
					System.out.print("*");
			}
			System.out.println();
			}
	}
}
}
